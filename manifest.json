{
    "name": "zoom detail",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "details"
    },
    "permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "src/js/*"
    ]
}